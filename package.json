{
    "name": "whynot",
    "description": "A super simple, trimed down, text first theme",
    "version": "v1.1.0",
    "engines": {
        "ghost": ">=3.0.0",
        "ghost-api": "v3"
    },
    "license": "MIT",
    "author": {
        "name": "Thomas",
        "email": "whynotavialable@protonmail.com",
        "url": "https://www.whynotavailable.com"
    },
    "gpm": {
        "type": "theme",
        "categories": [
            "Minimal"
        ]
    },
    "keywords": [
        "ghost",
        "theme",
        "ghost-theme"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/TryGhost/Casper.git"
    },
    "browserslist": [
        "defaults"
    ],
    "config": {
        "posts_per_page": 12
    },
    "dependencies": {
        "sass": "^1.26.5"
    },
    "scripts": {
        "styles": "sass styles/main.scss assets/css/screen.css",
        "styles-w": "sass --watch styles/main.scss assets/css/screen.css"
    }
}
